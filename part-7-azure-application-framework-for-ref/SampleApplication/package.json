{
  "name": "SampleApplication",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func host start",
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "coverage": "jest --coverage --coverageReporters=cobertura"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "node",
    "Azure",
    "es6",
    "Function"
  ],
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "desc": "Create Function App",
  "dependencies": {
    "@azure/cosmos": "3.7.4",
    "@hapi/joi": "17.1.1",
    "@types/node": "14.0.14",
    "axios": "0.19.2",
    "azure-middleware": "1.0.1",
    "jest-html-reporter": "3.1.3",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@azure/functions": "1.2.2",
    "@types/jest": "26.0.3",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.1",
    "typescript": "3.9.5"
  }
}
